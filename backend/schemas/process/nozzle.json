{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Nozzle",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "title": "ID", 
      "description": "Unique nozzle identifier"
    },
    "name": {
      "type": "string",
      "title": "Name",
      "description": "Nozzle name"
    },
    "manufacturer": {
      "type": "string",
      "title": "Manufacturer",
      "description": "Nozzle manufacturer"
    },
    "type": {
      "type": "string",
      "title": "Type",
      "enum": [
        "convergent-divergent",
        "convergent",
        "vented",
        "flat-plate",
        "de laval"
      ],
      "description": "Nozzle type"
    },
    "description": {
      "type": "string",
      "title": "Description",
      "description": "Nozzle description"
    }
  },
  "required": ["name", "type", "manufacturer"]
} 